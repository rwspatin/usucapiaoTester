<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <servlet>
        <servlet-name>UsucapiaoExtrajudicialServlet</servlet-name>
        <servlet-class>controller.UsucapiaoExtrajudicialServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsucapiaoJudicialExtraordinarioServlet</servlet-name>
        <servlet-class>controller.UsucapiaoJudicialExtraordinarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsucapiaoJudicialFamiliarServlet</servlet-name>
        <servlet-class>controller.UsucapiaoJudicialFamiliarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsucapiaoJudicialOrdinarioServlet</servlet-name>
        <servlet-class>controller.UsucapiaoJudicialOrdinarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsucapiaoJudicialRuralServlet</servlet-name>
        <servlet-class>controller.UsucapiaoJudicialRuralServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsucapiaoJudicialUrbanoColetivoServlet</servlet-name>
        <servlet-class>controller.UsucapiaoJudicialUrbanoColetivoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsucapiaoJudicialUrbanoServlet</servlet-name>
        <servlet-class>controller.UsucapiaoJudicialUrbanoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsucapiaoExtrajudicialServlet</servlet-name>
        <url-pattern>/UsucapiaoExtrajudicialServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsucapiaoJudicialExtraordinarioServlet</servlet-name>
        <url-pattern>/UsucapiaoJudicialExtraordinarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsucapiaoJudicialFamiliarServlet</servlet-name>
        <url-pattern>/UsucapiaoJudicialFamiliarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsucapiaoJudicialOrdinarioServlet</servlet-name>
        <url-pattern>/UsucapiaoJudicialOrdinarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsucapiaoJudicialRuralServlet</servlet-name>
        <url-pattern>/UsucapiaoJudicialRuralServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsucapiaoJudicialUrbanoColetivoServlet</servlet-name>
        <url-pattern>/UsucapiaoJudicialUrbanoColetivoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsucapiaoJudicialUrbanoServlet</servlet-name>
        <url-pattern>/UsucapiaoJudicialUrbanoServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>
            index.jsp
        </welcome-file>
    </welcome-file-list>
</web-app>
